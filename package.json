    "babel-jest": "^27.3.1",
    "commitizen": "^4.2.4",
    "concurrently": "^6.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-cofe": "^0.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
